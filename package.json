{
  "name": "rempl-webpack-analyzer",
  "version": "0.1.5",
  "description": "Webpack plugin and UI for analyzing webpack building process through rempl",
  "scripts": {
    "codestyle": "node node_modules/jscs/bin/jscs src && node node_modules/eslint/bin/eslint.js -c ./.eslintrc src",
    "build:dev": "node node_modules/basisjs-tools/bin/basis build",
    "build": "node node_modules/basisjs-tools/bin/basis build --pack --css-usage --css-optimize-names --js-cut-dev",
    "start": "node node_modules/http-server/bin/http-server ."
  },
  "main": "./src/plugin/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smelukov/rempl-webpack-analyzer.git"
  },
  "contributors": [
    "Sergey Melyukov <s.melukov@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smelukov/rempl-webpack-analyzer/issues"
  },
  "homepage": "https://github.com/smelukov/rempl-webpack-analyzer#readme",
  "dependencies": {
    "rempl": "^1.0.0-alpha3",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "basisjs": "^1.10.0",
    "basisjs-tools": "^1.10.2",
    "eslint": "^3.11.1",
    "http-server": "^0.9.0",
    "jscs": "^3.0.7"
  }
}
