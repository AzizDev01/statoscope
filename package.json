{
  "name": "webpack-runtime-analyzer",
  "version": "1.0.0-alpha2",
  "description": "Webpack plugin for analyzing internal processes, state and structure of bundles",
  "scripts": {
    "prepublish": "npm run build",
    "codestyle": "jscs src && eslint -c ./.eslintrc src",
    "build": "basis build --pack --css-usage --css-pack --js-cut-dev",
    "start": "http-server ."
  },
  "main": "./src/plugin/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smelukov/webpack-runtime-analyzer.git"
  },
  "contributors": [
    "Sergey Melyukov <s.melukov@gmail.com>"
  ],
  "keywords": [
    "webpack",
    "runtime",
    "building",
    "rempl",
    "remote",
    "devtool"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smelukov/webpack-runtime-analyzer/issues"
  },
  "homepage": "https://github.com/smelukov/webpack-runtime-analyzer#readme",
  "dependencies": {
    "rempl": "1.0.0-alpha5",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "basisjs": "^1.10.0",
    "basisjs-tools": "^1.10.2",
    "eslint": "^3.11.1",
    "http-server": "^0.9.0",
    "jscs": "^3.0.7"
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ]
}
